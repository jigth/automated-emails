#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/ts-node@9.1.1_typescript@4.2.4/node_modules/ts-node/dist/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/ts-node@9.1.1_typescript@4.2.4/node_modules/ts-node/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/ts-node@9.1.1_typescript@4.2.4/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules:/home/jigth/myCode/my-personal-projects/node_modules:/home/jigth/myCode/node_modules:/home/jigth/node_modules:/home/node_modules:/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/ts-node/dist/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/ts-node/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-script-deprecated.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin-script-deprecated.js" "$@"
fi
