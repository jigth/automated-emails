#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/rimraf@3.0.2/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules:/home/jigth/myCode/my-personal-projects/node_modules:/home/jigth/myCode/node_modules:/home/jigth/node_modules:/home/node_modules:/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/istanbul-lib-processinfo@2.0.2/node_modules/istanbul-lib-processinfo/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/istanbul-lib-processinfo@2.0.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rimraf@3.0.2/node_modules/rimraf/bin.js" "$@"
else
  exec node  "$basedir/../../../../../rimraf@3.0.2/node_modules/rimraf/bin.js" "$@"
fi
