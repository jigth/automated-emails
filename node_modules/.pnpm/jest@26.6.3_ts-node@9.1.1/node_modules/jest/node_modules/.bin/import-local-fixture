#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/import-local@3.0.2/node_modules/import-local/fixtures/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/import-local@3.0.2/node_modules/import-local/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/import-local@3.0.2/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules:/home/jigth/myCode/my-personal-projects/node_modules:/home/jigth/myCode/node_modules:/home/jigth/node_modules:/home/node_modules:/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/jest@26.6.3_ts-node@9.1.1/node_modules/jest/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/jest@26.6.3_ts-node@9.1.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../import-local@3.0.2/node_modules/import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../../../../../import-local@3.0.2/node_modules/import-local/fixtures/cli.js" "$@"
fi
