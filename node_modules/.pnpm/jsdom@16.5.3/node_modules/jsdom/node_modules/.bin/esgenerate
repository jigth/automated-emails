#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/bin/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/escodegen@2.0.0/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules:/home/jigth/myCode/my-personal-projects/node_modules:/home/jigth/myCode/node_modules:/home/jigth/node_modules:/home/node_modules:/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/jsdom@16.5.3/node_modules/jsdom/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/jsdom@16.5.3/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../escodegen@2.0.0/node_modules/escodegen/bin/esgenerate.js" "$@"
else
  exec node  "$basedir/../../../../../escodegen@2.0.0/node_modules/escodegen/bin/esgenerate.js" "$@"
fi
