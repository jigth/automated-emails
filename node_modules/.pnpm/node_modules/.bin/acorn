#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/acorn@8.2.4/node_modules/acorn/bin/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/acorn@8.2.4/node_modules/acorn/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/acorn@8.2.4/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules:/home/jigth/myCode/my-personal-projects/node_modules:/home/jigth/myCode/node_modules:/home/jigth/node_modules:/home/node_modules:/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/node_modules/acorn/bin/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/node_modules/acorn/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
