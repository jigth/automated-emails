#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/@cnakazawa+watch@1.0.4/node_modules/@cnakazawa/watch/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/@cnakazawa+watch@1.0.4/node_modules/@cnakazawa/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/@cnakazawa+watch@1.0.4/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules:/home/jigth/myCode/my-personal-projects/node_modules:/home/jigth/myCode/node_modules:/home/jigth/node_modules:/home/node_modules:/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/sane@4.1.0/node_modules/sane/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/sane@4.1.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@cnakazawa+watch@1.0.4/node_modules/@cnakazawa/watch/cli.js" "$@"
else
  exec node  "$basedir/../../../../../@cnakazawa+watch@1.0.4/node_modules/@cnakazawa/watch/cli.js" "$@"
fi
