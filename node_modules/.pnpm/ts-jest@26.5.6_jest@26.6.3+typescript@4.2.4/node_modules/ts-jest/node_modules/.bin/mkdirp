#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/mkdirp@1.0.4/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules:/home/jigth/myCode/my-personal-projects/node_modules:/home/jigth/myCode/node_modules:/home/jigth/node_modules:/home/node_modules:/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/ts-jest@26.5.6_jest@26.6.3+typescript@4.2.4/node_modules/ts-jest/node_modules:/home/jigth/myCode/my-personal-projects/automated-email-sender/node_modules/.pnpm/ts-jest@26.5.6_jest@26.6.3+typescript@4.2.4/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
fi
